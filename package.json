{
  "name": "le3-jam-starter",
  "scripts": {
    "start": "run-s build serve",
    "serve": "http-server ./dist",
    "build": "npm-run-all --sequential clean --parallel build:*",
    "clean": "rimraf ./dist",
    "build:html": "cross-env ELEVENTY_ENV=production eleventy",
    "build:assets": "cross-env NODE_ENV=production parcel build ./src/assets/app.css ./src/assets/app.ts --out-dir ./dist/assets"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.4.3",
    "typescript": "^3.8.3",
    "http-server": "^0.12.3"
  }
}